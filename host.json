{
  "version": "2.0",
  "functionTimeout": "-1",
  "logging": {
    "logLevel": {
      "default": "Information",
      "Host.Aggregator": "Information",
      "Host.Results": "Information",
      "Function": "Information"
    },
    "applicationInsights": {
      "enableLiveMetrics": true
    }      
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "managedDependency": {
    "enabled": true
  }
}
